                        <span class="c">&lt;!-- disabled 禁用，不能修改，提交时也不会被传输 --&gt;</span>  
                        <span class="nt">&lt;input</span> <span class="na">type=</span><span class="s">"text"</span> <span class="na">name=</span><span class="s">"username"</span> <span class="na">maxlength=</span><span class="s">"10"</span> <span class="na">readonly</span> <span class="na">value=</span><span class="s">"初始值"</span><span class="nt">&gt;</span>         
