                <span class="nt">&lt;/select&gt;</span>  
            <span class="nt">&lt;/p&gt;</span>  
      
            <span class="c">&lt;!-- 单选 --&gt;</span>  
            <span class="nt">&lt;p&gt;</span>单选<span class="nt">&lt;/p&gt;</span>  
            <span class="nt">&lt;p&gt;</span>  
                <span class="c">&lt;!-- label 的作用是在点击文字部分也能选择 --&gt;</span>  
                <span class="nt">&lt;label&gt;&lt;input</span> <span class="na">type=</span><span class="s">"radio"</span> <span class="na">name=</span><span class="s">"sex"</span><span class="nt">&gt;</span> 男 <span class="nt">&lt;/label&gt;</span>  
                <span class="nt">&lt;label&gt;&lt;input</span> <span class="na">type=</span><span class="s">"radio"</span> <span class="na">name=</span><span class="s">"sex"</span><span class="nt">&gt;</span> 女 <span class="nt">&lt;/label&gt;</span>  
            <span class="nt">&lt;/p&gt;</span>  
      
            <span class="c">&lt;!-- 多选 --&gt;</span>  
            <span class="nt">&lt;p&gt;</span>多选<span class="nt">&lt;/p&gt;</span>  
            <span class="nt">&lt;p&gt;</span>  
                <span class="nt">&lt;label&gt;&lt;input</span> <span class="na">type=</span><span class="s">"checkbox"</span> <span class="na">name=</span><span class="s">"love[]"</span><span class="nt">&gt;</span>篮球<span class="nt">&lt;/label&gt;</span>  
                <span class="nt">&lt;label&gt;&lt;input</span> <span class="na">type=</span><span class="s">"checkbox"</span> <span class="na">name=</span><span class="s">"love[]"</span><span class="nt">&gt;</span>排球<span class="nt">&lt;/label&gt;</span>  
                <span class="nt">&lt;label&gt;&lt;input</span> <span class="na">type=</span><span class="s">"checkbox"</span> <span class="na">name=</span><span class="s">"love[]"</span><span class="nt">&gt;</span>羽毛球<span class="nt">&lt;/label&gt;</span>  
                <span class="nt">&lt;label&gt;&lt;input</span> <span class="na">type=</span><span class="s">"checkbox"</span> <span class="na">name=</span><span class="s">"love[]"</span><span class="nt">&gt;</span>乒乓球<span class="nt">&lt;/label&gt;</span>  
            <span class="nt">&lt;/p&gt;</span>  
      
            <span class="nt">&lt;p&gt;</span>  
                <span class="nt">&lt;input</span> <span class="na">type=</span><span class="s">"submit"</span> <span class="na">value=</span><span class="s">"提交"</span><span class="nt">&gt;</span>  
                <span class="nt">&lt;input</span> <span class="na">type=</span><span class="s">"reset"</span> <span class="na">value=</span><span class="s">"取消"</span><span class="nt">&gt;</span>  
            <span class="nt">&lt;/p&gt;</span>  
      
