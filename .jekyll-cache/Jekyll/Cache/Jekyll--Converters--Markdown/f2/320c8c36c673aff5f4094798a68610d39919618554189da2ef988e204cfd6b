            <span class="nt">&lt;p&gt;</span>  
                <span class="nt">&lt;select</span> <span class="na">name=</span><span class="s">"城市"</span><span class="nt">&gt;</span>  
                    <span class="nt">&lt;option</span> <span class="na">value=</span><span class="s">""</span><span class="nt">&gt;</span>请选择<span class="nt">&lt;/option&gt;</span>  
