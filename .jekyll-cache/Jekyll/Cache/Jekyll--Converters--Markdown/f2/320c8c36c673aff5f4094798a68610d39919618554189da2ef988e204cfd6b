        <span class="nt">&lt;form</span> <span class="na">action=</span><span class="s">"zhuce.php"</span> <span class="na">method=</span><span class="s">"post"</span><span class="nt">&gt;</span>  
            <span class="nt">&lt;table</span> <span class="na">border=</span><span class="s">"0px"</span> <span class="na">cellspacing=</span><span class="s">"0px"</span> <span class="na">width=</span><span class="s">"300px"</span><span class="nt">&gt;</span>  
