                <span class="nt">&lt;input</span> <span class="na">type=</span><span class="s">"file"</span> <span class="na">name=</span><span class="s">"文件上传"</span><span class="nt">&gt;</span>  
            <span class="nt">&lt;/p&gt;</span>  
