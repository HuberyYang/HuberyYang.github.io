        <span class="nt">&lt;meta</span> <span class="na">charset=</span><span class="s">"utf-8"</span><span class="nt">&gt;</span>  
        <span class="nt">&lt;style </span><span class="na">type=</span><span class="s">"text/css"</span><span class="nt">&gt;</span>  
            <span class="o">*</span><span class="p">{</span>  
                <span class="nl">font-family</span><span class="p">:</span> <span class="err">微软雅黑</span><span class="p">;</span>  
                <span class="nl">text-align</span><span class="p">:</span> <span class="nb">left</span><span class="p">;</span>  
                <span class="nl">margin</span><span class="p">:</span> <span class="m">5px</span><span class="p">;</span>  
            <span class="p">}</span>  
      
            <span class="nt">td</span><span class="p">{</span>  
                <span class="nl">height</span><span class="p">:</span> <span class="m">30px</span><span class="p">;</span>  
            <span class="p">}</span>  
      
            <span class="nt">textarea</span><span class="p">{</span>  
                <span class="c">/*关闭文本域的重置大小的能力*/</span>  
                <span class="nl">resize</span><span class="p">:</span> <span class="nb">none</span><span class="p">;</span>  
            <span class="p">}</span>  
      
